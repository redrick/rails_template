{
  "presets": [
    ["@babel/env", {
      "modules": false,
      "targets": {
        "browsers": "> 1%",
      },
      "useBuiltIns": "entry",
      "forceAllTransforms": true
    }]
  ],

  "plugins": [
    "@babel/syntax-dynamic-import",
    ["transform-class-properties", { "spec": true }]
  ]
}
